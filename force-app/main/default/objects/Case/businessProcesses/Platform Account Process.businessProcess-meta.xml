<?xml version="1.0" encoding="UTF-8"?>
<BusinessProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Platform Account Process</fullName>
    <description>In order to create Account in Platform create a case as a request for Data Admins</description>
    <isActive>true</isActive>
    <values>
        <fullName>Closed Rejected</fullName>
        <default>false</default>
    </values>
    <values>
        <fullName>Closed Resolved</fullName>
        <default>false</default>
    </values>
    <values>
        <fullName>New %7C Unreviewed</fullName>
        <default>true</default>
    </values>
    <values>
        <fullName>Under Review %7C Awaiting Admin</fullName>
        <default>false</default>
    </values>
</BusinessProcess>
