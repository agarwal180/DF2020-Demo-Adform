    <actionOnUpdate>Block</actionOnUpdate>
    <alertText>Duplicate Records</alertText>
    <description>Preventing Users to create Duplicate Account based on Legal Company Name and Registered Number</description>
    <duplicateRuleFilter xsi:nil="true"/>
    <duplicateRuleMatchRules>
        <matchRuleSObjectType>Account</matchRuleSObjectType>
        <matchingRule>Prevent_Account_Duplicate</matchingRule>
        <objectMapping xsi:nil="true"/>
    </duplicateRuleMatchRules>
    <isActive>true</isActive>
    <masterLabel>Prevent Account Duplicates</masterLabel>
    <securityOption>EnforceSharingRules</securityOption>
    <sortOrder>2</sortOrder>
</DuplicateRule>
